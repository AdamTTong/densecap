train_net: "models/faster_rcnn_cap/train_cap.prototxt"
base_lr: 0.01
lr_policy: "step"
gamma: 0.5
stepsize: 50000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "faster_rcnn_cap"
iter_size: 2
clip_gradients: 100